{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "produced",
  "title": "Produced VC",
  "type": "object",
  "required": ["@context", "type", "issuer", "issuanceDate", "credentialSubject", "proof"],
  "properties": {
    "@context": {"type": ["string", "array"]},
    "type": {"type": "array", "items": {"enum": ["VerifiableCredential", "ProducedVC"]}},
    "issuer": {"type": "string", "format": "uri"},
    "issuanceDate": {"type": "string", "format": "date-time"},
    "credentialSubject": {
      "type": "object",
      "required": ["owner", "owned"]
    ,"properties": {
        "owner": {"type": "string", "format": "uri"},
        "owned": {"type": "string"}
      }
    },
    "proof": {"type": "object"}
  }
}